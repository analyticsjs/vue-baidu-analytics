#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_semver@6.3.0@semver/bin/semver.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../_semver@6.3.0@semver/bin/semver.js" "$@"
  ret=$?
fi
exit $ret
